	@(dir=.; test -f ./netcdf-f77.texi || dir=$(srcdir); \
	set `$(SHELL) $(srcdir)/mdate-sh $$dir/netcdf-f77.texi`; \
	echo "@set UPDATED $$1 $$2 $$3"; \
	echo "@set UPDATED-MONTH $$2 $$3"; \
	echo "@set EDITION $(VERSION)"; \
	echo "@set VERSION $(VERSION)") > vti.tmp
	@cmp -s vti.tmp $(srcdir)/version-f77.texi \
	  || (echo "Updating $(srcdir)/version-f77.texi"; \
	      cp vti.tmp $(srcdir)/version-f77.texi)
	-@rm -f vti.tmp
	@cp $(srcdir)/version-f77.texi $@

